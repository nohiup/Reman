                    android:textSize="35sp"
                    app:layout_constraintBottom_toBottomOf="@+id/table_11_img"
                    app:layout_constraintEnd_toEndOf="@+id/table_11_img"
                    app:layout_constraintStart_toStartOf="@+id/table_11_img"
                    app:layout_constraintTop_toTopOf="@+id/table_11_img"

                    android:fontFamily="@font/quicksand_medium"
                    android:text="12"
                    android:textColor="@color/white"
                    android:textSize="35sp"
                    app:layout_constraintBottom_toBottomOf="@+id/table_12_img"
                    app:layout_constraintEnd_toEndOf="@+id/table_12_img"
                    app:layout_constraintStart_toStartOf="@+id/table_12_img"
